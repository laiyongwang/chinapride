<p id="notice"><%= notice %></p>


  <h2 class="category_title">All <%= @category.name %></h2>
  <ul class="browse-auctions">
  <% @category.products.each do |product| %>
	<li>
	<%= image_tag product.photo.url, :size => "200x200" %> <br />
	<%= product.name %> <br />
	<%= product.content%> <br />
    </li>
  <% end %>
  </ul>


<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
